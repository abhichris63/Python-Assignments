    "            if i.endswith('.jpg') or i.endswith('.pdf'):\n",
    "                k1 = str(i)\n",
    "#                 print(i)\n",
    "                source = k1\n",
    "                print(str(source))\n",
    "                destination = r\"C:\\Users\\ADMIN\\Desktop\\newFilesInJPG_PDFFormate\"\n",
    "                copying_files = shutil.copy(os.path.abspath(root + '/' + str(i)), destination)\n",
    "                print(copying_files)\n",
    "\n",
    "    \n",
    "a= file_dir()"
